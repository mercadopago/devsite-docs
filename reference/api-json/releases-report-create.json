{
    "url": "https://api.mercadopago.com",
    "paths": {
        "/v1/account/release_report": {
            "post": {
              "tags": [
                "mp-point",
                "mp-point"
              ],
              "description": {
                "en": "",
                "pt": "",
                "es": "generar manualmente un nuevo reporte dentro de un intervalo de fechas específico."
              },
              "parameters": [
                {
                  "name": "Authorization",
                  "in": "header",
                  "required": true,
                  "description": {
                    "en": "",
                    "pt": "",
                    "es": "Token necesario para autenticación. Este campo está destinado a plataformas o módulos que ofrecen Mercado Pago en sus soluciones."
                  },
                  "schema": {
                    "type": "string",
                    "example": "Bearer <TOKEN>"
                  }
                }
              ],
              "requestBody": {
                "description": {
                  "en": "",
                  "pt": "",
                  "es": "Estos son los campos necesarios para generar manualmente un nuevo reporte"
                },
                "content": {
                  "application/json": {
                    "schema": {
                      "type": "object",
                      "properties": {
                        "begin_date": {
                          "type": "string",
                          "required": true,
                          "description": {
                            "en": "",
                            "pt": "",
                            "es": "Este campo indica la fecha y hora de inicio del intervalo para el reporte.en el huso horario UTC (indicado por la 'Z' al final, que significa \"Zulu time\" o tiempo coordinado universal)"
                          },
                          "example": "2019-05-01T00:00:00Z"
                        },
                        "end_date": {
                          "type": "string",
                          "required": true,
                          "description": {
                            "en": "",
                            "pt": "",
                            "es": "Este campo indica  la fecha y hora de finalización del intervalo para el reporte.en el huso horario UTC (indicado por la 'Z' al final, que significa \"Zulu time\" o tiempo coordinado universal)"
                          },
                          "example": "2019-06-01T00:00:00Z"
                        }
                      }
                    }
                  }
                }
              },
              "responses": {
                "202": {
                  "description": {
                    "en": "Accepted",
                    "pt": "",
                    "es": "Accepted"
                  },
                  "content": {
                    "application/json": {
                      "schema": {
                        "description": {
                          "en": "",
                          "pt": "",
                          "es": "El API responderá con una estructura JSON con información relevante a tu pedido de generación"
                        },
                        "type": "object",
                        "properties": {
                          "id": {
                            "type": "number",
                            "description": {
                              "en": "",
                              "pt": "",
                              "es": "Este campo indica el identificador único del reporte generado."
                            },
                            "example": 99336983670
                          },
                          "account_id": {
                            "type": "number",
                            "description": {
                              "es": "Este campo indica el identificador único de la cuenta asociada al reporte.",
                              "pt": "",
                              "en": ""
                            },
                            "example": 167345291
                          },
                          "begin_date": {
                            "type": "string",
                            "description": {
                              "es": "Es la fecha y hora de inicio del intervalo del reporte.",
                              "pt": "",
                              "en": ""
                            },
                            "example": "2022-08-01T03:00:00Z"
                          },
                          "created_from": {
                            "type": "string",
                            "description": {
                              "es": "Indica cómo se generó el reporte. En este caso",
                              "pt": "",
                              "en": ""
                            },
                            "example": "manual"
                          },
                          "currency_id": {
                            "type": "string",
                            "description": {
                              "es": "Es el identificador de la moneda utilizada en el reporte.",
                              "pt": "",
                              "en": ""
                            },
                            "example": "ARS"
                          },
                          "end_date": {
                            "type": "string",
                            "description": {
                              "es": "Representa la fecha y hora de finalización del intervalo del reporte.",
                              "pt": "",
                              "en": ""
                            },
                            "example": "2022-08-08T02:59:59Z"
                          },
                          "generation_date": {
                            "type": "string",
                            "description": {
                              "es": "Es la fecha y hora en la que se generó la respuesta del reporte",
                              "pt": "",
                              "en": ""
                            },
                            "example": "2024-02-16T15:55:22.258-04:00"
                          },
                          "internal_management": {
                            "type": "array",
                            "items": {
                              "type": "object",
                              "description": {
                                "en": "",
                                "pt": "",
                                "es": "Lista que contiene información interna de gestión del reporte, como la visibilidad, notificaciones y configuraciones de tiempo."
                              },
                              "properties": {
                                "is_visible": {
                                  "type": "boolean",
                                  "description": {
                                    "en": "",
                                    "pt": "",
                                    "es": "Indica si el reporte es visible o no"
                                  },
                                  "example": true
                                },
                                "notify": {
                                  "type": "boolean",
                                  "description": {
                                    "en": "",
                                    "pt": "",
                                    "es": "Controla si se notificará al correo electrónico asociado sobre eventos relacionados con el reporte."
                                  },
                                  "example": false
                                },
                                "use_exact_time": {
                                  "type": "boolean",
                                  "description": {
                                    "en": "",
                                    "pt": "",
                                    "es": "Determina si se utilizarán fechas y horas exactas en el reporte"
                                  },
                                  "example": true
                                }
                              }
                            }
                          },
                          "is_reserve": {
                            "type": "boolean",
                            "description": {
                              "es": "Indica si el reporte es una reserva o no. ",
                              "pt": "",
                              "en": ""
                            },
                            "example": false
                          },
                          "is_test": {
                            "type": "boolean",
                            "description": {
                              "es": "Indica si el reporte es de prueba o no ",
                              "pt": "",
                              "en": ""
                            },
                            "example": false
                          },
                          "last_modified": {
                            "type": "string",
                            "description": {
                              "es": "Es la fecha y hora de la última modificación en el reporte.",
                              "pt": "",
                              "en": ""
                            },
                            "example": "2024-02-16T15:55:22.261-04:00"
                          },
                          "report_id": {
                            "type": "number",
                            "description": {
                              "es": "Es el identificador del reporte generado, si está disponible",
                              "pt": "",
                              "en": ""
                            },
                            "example": 17012160
                          },
                          "retries": {
                            "type": "number",
                            "description": {
                              "es": "Representa el número de intentos realizados para generar el reporte",
                              "pt": "",
                              "en": ""
                            },
                            "example": 0
                          },
                          "status": {
                            "type": "string",
                            "description": {
                              "es": "Indica el estado del reporte",
                              "pt": "",
                              "en": ""
                            },
                            "example": "processed"
                          },
                          "sub_type": {
                            "type": "string",
                            "description": {
                              "es": "Indica el subtipo de reporte",
                              "pt": "",
                              "en": ""
                            },
                            "example": "release"
                          },
                          "user_id": {
                            "type": "number",
                            "description": {
                              "es": "Es el identificador único del usuario asociado al reporte",
                              "pt": "",
                              "en": ""
                            },
                            "example": 123456789
                          },
                          "format": {
                            "type": "string",
                            "description": {
                              "es": "Indica el formato del reporte generado",
                              "pt": "",
                              "en": ""
                            },
                            "example": "CSV"
                          }
                        }
                      }
                    }
                  }
                },
                "400": {
                  "description": {
                    "en": "Bad request",
                    "pt": "Bad request",
                    "es": "Bad request"
                  },
                  "content": {
                    "application/json": {
                      "schema": {
                        "type": "object",
                        "properties": {
                          "errorKey": {
                            "type": "string",
                            "enum": [
                              {
                                "title": "invalid_begin_date",
                                "description": {
                                  "en": "",
                                  "es": "Must specify begin_date parameter",
                                  "pt": ""
                                }
                              },
                              {
                                "title": "invalid_end_date",
                                "description": {
                                  "en": "",
                                  "es": "Must specify end_date parameter",
                                  "pt": ""
                                }
                              },
                              {
                                "title": "end_date_before_begin_date",
                                "description": {
                                  "en": "",
                                  "es": "Begin date should be before end date",
                                  "pt": ""
                                }
                              },
                              {
                                "title": "end_date_before_begin_date",
                                "description": {
                                  "en": "",
                                  "es": "Begin date should be before end date",
                                  "pt": ""
                                }
                              }
                            ]
                          }
                        }
                      }
                    }
                  }
                },
                "401": {
                  "description": {
                    "en": "Unauthorized",
                    "pt": "Unauthorized",
                    "es": "Unauthorized"
                  },
                  "content": {
                    "application/json": {
                      "schema": {
                        "type": "object",
                        "properties": {
                          "errorKey": {
                            "type": "string",
                            "enum": [
                              {
                                "title": "invalid token",
                                "description": {
                                  "en": "",
                                  "es": "invalid token",
                                  "pt": ""
                                }
                              }
                            ]
                          }
                        }
                      }
                    }
                  }
                }
              }
            },
            "title": {
              "en": "",
              "pt": "",
              "es": "Crear reporte"
            }
        }
    }
  }